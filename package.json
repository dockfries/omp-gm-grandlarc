{
  "name": "omp-gm-grandlarc",
  "version": "1.0.0",
  "private": true,
  "description": "The GrandLarceny sample gamemode written with omp-node-starter",
  "keywords": [
    "sa-mp",
    "sa:mp",
    "open.mp",
    "omp",
    "gamemode",
    "samp-node"
  ],
  "homepage": "https://github.com/dockfries/omp-gm-grandlarc#readme",
  "bugs": {
    "url": "https://github.com/dockfries/omp-gm-grandlarc/issues"
  },
  "license": "ISC",
  "author": "dockfries",
  "scripts": {
    "serve": "omp-server",
    "dev": "npm-run-all -p -r -l dev:server dev:watch",
    "dev:watch": "cross-env NODE_ENV=dev rollup -c -w",
    "dev:server": "nodemon -x omp-server -w dist/bundle.js -d 2",
    "build": "npm run clean && cross-env NODE_ENV=prod rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint src/* --fix -c .eslintrc",
    "prepare": "husky install"
  },
  "dependencies": {
    "@infernus/core": "^0.11.0-beta.7",
    "@infernus/fs": "^0.11.1",
    "pino": "^9.1.0",
    "pino-pretty": "^11.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sa-mp/node": "^0.2.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "iconv-lite": "^0.6.3",
    "nodemon": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.2",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "typescript": "^5.4.5"
  }
}
